{
  "name": "blog",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack --mode production --optimization-minimize",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "sass ./css/main.scss ./_site/main.css --no-source-map --style=compressed",
    "watch:webpack": "webpack --watch --mode development",
    "watch:sass": "sass ./css/main.scss ./_site/main.css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve --quiet",
    "start": "cross-env NODE_ENV=dev run-p 'watch:*'",
    "build": "cross-env NODE_ENV=production run-s 'build:*'"
  },
  "author": "Max Kohler",
  "license": "ISC",
  "homepage": "/",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@codemirror/basic-setup": "^0.19.1",
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/view": "^0.19.40",
    "codemirror": "^5.65.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-typeset": "https://github.com/liamfiddler/eleventy-plugin-typeset.git#commit=6b99f063f7b0e308830f91df33aa6124e47a05a8",
    "liquidjs": "^9.32.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.3.1",
    "sass": "^1.49.0",
    "text-balancer": "^1.0.5",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "packageManager": "yarn@3.1.1"
}
