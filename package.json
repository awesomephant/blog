{
  "name": "blog",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack --mode production",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "sass ./css/main.scss ./_site/main.css --no-source-map",
    "watch:webpack": "webpack --watch --mode development",
    "watch:sass": "sass ./css/main.scss ./_site/main.css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve --quiet",
    "start": "cross-env NODE_ENV=dev run-p 'watch:*'",
    "build": "cross-env NODE_ENV=production run-s 'build:*'"
  },
  "author": "Max Kohler",
  "license": "ISC",
  "homepage": "/",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "@codemirror/basic-setup": "^0.19.0",
    "@codemirror/lang-html": "^0.19.3",
    "@codemirror/view": "^0.19.20",
    "codemirror": "^5.64.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-typeset": "github:liamfiddler/eleventy-plugin-typeset",
    "liquidjs": "^9.28.5",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.3.1",
    "sass": "^1.39.2",
    "text-balancer": "^1.0.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "packageManager": "yarn@3.1.1"
}
