{
  "name": "blog",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:webpack": "webpack --mode production --optimization-minimize",
    "build:eleventy": "npx @11ty/eleventy",
    "build:sass": "sass ./css/main.scss ./_site/main.css --no-source-map --style=compressed",
    "watch:webpack": "webpack --watch --mode development",
    "watch:sass": "sass ./css/main.scss ./_site/main.css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve --quiet",
    "start": "cross-env NODE_ENV=dev run-p 'watch:*'",
    "build": "cross-env NODE_ENV=production run-s 'build:*'"
  },
  "author": "Max Kohler",
  "license": "ISC",
  "homepage": "/",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "cross-env": "^7.0.3",
    "eleventy-plugin-typeset": "https://github.com/liamfiddler/eleventy-plugin-typeset.git",
    "liquidjs": "^9.36.0",
    "markdown-it-anchor": "^8.5.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-task-lists": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.50.0",
    "simple-git": "^3.5.0",
    "text-balancer": "^1.0.5",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "packageManager": "yarn@3.1.1"
}
